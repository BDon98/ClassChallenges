#include <iostream>

using namespace std;

int main() {
  cout << "Hello, please enter a number greater than 1" << endl;
  
  int number;
  cin >> number;
  
  for(int i = 1; i < number; i++)
  {
    if(i%3==0 && i%5==0)
    {
      cout << "FizzBuzz" << endl;
    }
    else if(i%3==0)
    {
      cout << "Fizz" << endl;
    }
    else if (i%5==0)
    {
      cout << "Buzz" << endl;
    }
    else
    {
      cout << i << endl;
    }
  }
}
